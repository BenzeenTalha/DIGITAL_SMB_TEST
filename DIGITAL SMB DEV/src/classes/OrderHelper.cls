    // Set all opportunities associated with newly activated services to Closed won
    public static void closeWinParentOpp(List<String> orderGroupIdList) {
        try {
            List<Order_Group__c> orderGroupList = [select Id, Opportunity__c, (select Id, Order_Status__c from Orders__r) from Order_Group__c where id in :orderGroupIdList];
            //
            List<String> oppIdList = new List<String>();
            for (Order_Group__c orderGroup : orderGroupList) {
                Boolean closeWinOpp = true;
                for (Order__c order : orderGroup.Orders__r) {
                    if (order.Order_Status__c != 'Active') {
                        closeWinOpp = false;
                    }
                }
                if (closeWinOpp) oppIdList.add(orderGroup.Opportunity__c);
            }
            //
            if (!oppIdList.isEmpty())  {
                List<Opportunity> oppList = [select Id, StageName, CloseDate from Opportunity where Id in :oppIdList];
                for (Opportunity opp : oppList) {
                    opp.StageName = 'Closed Won';
                    opp.Close_Comment__c = 'Automatically closed won by system';

                    // RAC : Override Close Date if it's in the future
                    if (opp.CloseDate > System.today()) opp.CloseDate = System.today();
                }
                update oppList;
            }
        } catch(Exception ex) {
            throw new CustomValidationException('Can not close win the parent  opportunity of this order: '+ex);
        }
    }

    // Set all opportunities associated with newly activated services to Closed won
    public static void activateSecondaryOrders(List<String> parentOrderIdList) {
        try {
            List<Order__c> orderList = [select Id, (select Id, Order_Status__c from Secondary_Orders__r) from Order__c where id in :parentOrderIdList];
            List<Order__c> secondaryOrderList = new List<Order__c>();
            //
            for (Order__c order : orderList) {
                Boolean closeWinOpp = true;
                for (Order__c secondaryOrder : order.Secondary_Orders__r) {
                    secondaryOrder.Order_Status__c = 'Active';
                }
                if (!order.Secondary_Orders__r.isEmpty()) secondaryOrderList.addAll(order.Secondary_Orders__r);
            }
            //
            update secondaryOrderList;
        } catch(Exception ex) {
            throw new CustomValidationException('Can not activate secondary orders: '+ex);
        }
    }

    //update the order group summary information

    public static void countOrders(List <String> orderGroupIdList) {
        AggregateResult[] results = [select COUNT(id) total, Order_Group__c, Product_Category__c
                                       from Order__c
                                      where Order_Group__c in :orderGroupIdList GROUP BY  Order_Group__c, Product_Category__c ] ;
        //
        Map<String,Order_Group__c> orderGroupMap =
            new Map<String,Order_Group__c>([select Id, Number_of_Calls_Only_Orders__c, Number_of_Landline_orders__c,
                                                   Number_of_ISDN2_orders__c, Number_of_ISDN30_orders__c, Number_of_Telemarketing_orders__c,
                                                   Number_of_Business_Broadband_orders__c, Number_of_Mobile_orders__c
                                              from Order_Group__c where Id in :orderGroupIdList]);
        //
        Decimal countCallsOnly = 0;
        Decimal countLandline = 0;
        Decimal countISDN2 = 0;
        Decimal countISDN30 = 0;
        Decimal countTelemarketing = 0;
        Decimal countBusinessBroadband = 0;
        Decimal countMobile = 0;
        //
        for (AggregateResult ar : results) {
            if (orderGroupMap.containsKey((String)ar.get('Order_Group__c'))) {
                Order_Group__c orderGroup = orderGroupMap.get((String)ar.get('Order_Group__c'));
                if ((String)ar.get('Product_Category__c') == 'Calls Only') {
                    countCallsOnly = (Decimal)ar.get('total');
                } else if ((String)ar.get('Product_Category__c') == 'Landline') {
                    countLandline = (Decimal)ar.get('total');
                } else if ((String)ar.get('Product_Category__c') == 'ISDN2') {
                    countISDN2 = (Decimal)ar.get('total');
                } else if ((String)ar.get('Product_Category__c') == 'ISDN30') {
                    countISDN30 = (Decimal)ar.get('total');
                } else if ((String)ar.get('Product_Category__c') == 'Telemarketing') {
                    countTelemarketing = (Decimal)ar.get('total');
                } else if ((String)ar.get('Product_Category__c') == 'Business Broadband') {
                    countBusinessBroadband = (Decimal)ar.get('total');
                } else if ((String)ar.get('Product_Category__c') == 'Mobile') {
                    countMobile = (Decimal)ar.get('total');
                }
                //
                orderGroup.Number_of_Calls_Only_Orders__c = countCallsOnly;
                orderGroup.Number_of_Landline_orders__c = countLandline;
                orderGroup.Number_of_ISDN2_orders__c = countISDN2;
                orderGroup.Number_of_ISDN30_orders__c = countISDN30;
                orderGroup.Number_of_Telemarketing_orders__c = countTelemarketing;
                orderGroup.Number_of_Business_Broadband_orders__c = countBusinessBroadband;
                orderGroup.Number_of_Mobile_orders__c = countMobile;
                //
                orderGroupMap.put((String)ar.get('Order_Group__c'),orderGroup);
            }
        }
        update orderGroupMap.values();
    }

    public static List<CSPOFA__Orchestration_Process__c> createOrderGroupProcess(Id orderGroupId) {

        // For the given Order Group Id create the header level process. The steps will be auto-populated
        // by the package on creation of the process object.
        // NB: The calling function must insert the records!
        List<CSPOFA__Orchestration_Process__c> processList = new List<CSPOFA__Orchestration_Process__c>();

        String processName = 'Process for Order Groups';

       /* //Unify - Set different template if this is Unify Order Group
        Order_Group__c thisOrderGroup = [SELECT Opportunity__r.RecordTypeId FROM Order_Group__c where Id = :orderGroupId LIMIT 1];
        if(!Test.isRunningTest())
        {
	        if(thisOrderGroup.Opportunity__r.RecordTypeId == UnifyConfiguration__c.getInstance('UnifyOpportunityRecordTypeId').Value__c){
	        	processName = 'UNIFY Process for Order Group';
	        }
        }*/
        // added for new version of Orchestrator 1.51.
        if(Test.isRunningTest()){
       		CSPOFA__Orchestration_Process_Template__c pt = new CSPOFA__Orchestration_Process_Template__c(Name = 'Process for Order Groups', CSPOFA__Process_Type__c = 'New Provide');
    		insert pt;
        }
		Id templateId = [select Id from CSPOFA__Orchestration_Process_Template__c
		where  CSPOFA__Process_Type__c = 'New Provide'
		and Name = :processName LIMIT 1].Id;

        CSPOFA__Orchestration_Process__c orchProcess = new CSPOFA__Orchestration_Process__c(Name = processName, CSPOFA__Process_Type__c = 'New Provide', Order_Group__c = orderGroupId, CSPOFA__Orchestration_Process_Template__c = templateId);


        processList.add(orchProcess);

        return processList;
    }


    public static List<CSPOFA__Orchestration_Process__c> createOrderProcess(Id orderId, String processName){

    	// added for new version of Orchestrator 1.51.
		Id templateId = [select Id from CSPOFA__Orchestration_Process_Template__c
		where  CSPOFA__Process_Type__c = 'New Provide'
		and Name = :processName].Id;


    	List<CSPOFA__Orchestration_Process__c> processList = new List<CSPOFA__Orchestration_Process__c>();
        processList.add(new CSPOFA__Orchestration_Process__c(Name = processName,
                                                             CSPOFA__Process_Type__c = 'New Provide',
                                                             Order__c       = orderId,
                                                             CSPOFA__Orchestration_Process_Template__c = templateId));

		return processList;
    }

