    public static void pauseOrderSLAs (Order__c ord) {

    	List <SLA__c> slas = [select Id,Order__r.Id,Pause__c,Completed__c from SLA__c where Order__r.Id = : ord.Id and Completed__c = false and Pause__c = false];

    	for (integer i = 0; i < slas.size(); ++i) {
    		slas[i].Pause__c = true;
    	}

    	update slas;
    }

    public static void unpauseOrderSLAs (Order__c ord) {

    	List <SLA__c> slas = [select Id,Order__r.Id,Pause__c,Completed__c from SLA__c where Order__r.Id = : ord.Id and Completed__c = false and Pause__c = true];

    	for (integer i = 0; i < slas.size(); ++i) {
    		slas[i].Pause__c = false;
    	}

    	update slas;

    }

    public static void deleteSLAs (Order__c ord) {

    	List <SLA__c> slas = [select Id,Order__r.Id from SLA__c where Order__r.Id = : ord.Id];

    	delete slas;

    }

    public static void assignResource (Order__c ord) {

    	List <SLA__c> slas = [select Id,Assignee__c,Actual_Date__c from SLA__c where Order__c = : ord.Id and Completed__c = false];

    	for (integer i = 0; i < slas.size(); ++i) {
    		slas[i].Assignee__c = ord.Assignee__c;
    	}
		update slas;
    }

