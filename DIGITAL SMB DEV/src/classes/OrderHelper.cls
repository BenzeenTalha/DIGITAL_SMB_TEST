    // Check what type of order we have just created and check if a correspondiong template exists
    // If one does exist create the Asset lines and asset configuration lines for this sevrice based
    // on the template
    //@future
    public static void createAssetAndConfigLines(List<String> orderIdList) {

        String orderId;
        try {
            List <Product_Template__c> pTemplateList =
                [select Id, Product_Type__c, (select Id, Type__c, Requires_Orchestration__c from Asset_Template_Lines__r),
                (select Id, Type__c, Asset_Type__c, Related_Order_Field_Name__c from Asset_config_Template_Lines__r) //,
                //(select Id, Name, Process__c, Associated_Profile__c, Asset_Template_Line__r.Type__c,Product_Template__c from Orchestration_Process_Templates__r)
                from Product_Template__c];
            Map <String,Product_Template__c> pTemplateMap = new Map <String,Product_Template__c> ();
            for (Product_Template__c pTemplate : pTemplateList) {
                pTemplateMap.put(ptemplate.Product_Type__c,pTemplate);
            }
            String queryString = 'select '+Utility.getSobjectFields('Order__c')+
                ' from Order__c where Id in '+Utility.convertListToString(orderIdList);
            List <Order__c> orderList = Database.query(queryString);
            //
            // Create Asset and Configuration Lists to insert the new records
            List <Order_Asset__c> orderAssetList = new List <Order_Asset__c>();
            List <Asset_config_Template_Line__c> templateConfigList = new List <Asset_config_Template_Line__c> ();
            Map <String,List <Asset_config_Template_Line__c>> configTemplateMap = new Map  <String,List<Asset_config_Template_Line__c>> ();
            Map <ID,Order__c> orderMap = new Map <ID,Order__c> ();
            // Loop through all orders
            for (Integer i = 0; i < orderList.size(); i++) {
                orderId = orderList[i].Id;
                if (orderList[i].Order_Status_Reason__c != 'Novation') {
                    orderMap.put(orderList[i].Id,orderList[i]);
                    if (pTemplateMap.containsKey(orderList[i].product_type__c)) {
                        for (Asset_Template_Line__c asset : pTemplateMap.get(orderList[i].product_type__c).Asset_Template_Lines__r) {
                            String defaultAssetStatus;
                            if (orderList[i].Process_Orchestration_Enabled__c && !(asset.Requires_Orchestration__c)) {
                                defaultAssetStatus = 'Installed';
                            } else {
                                defaultAssetStatus = 'In Provisioning';
                            }
                            //
                            if (asset.Type__c == 'Line') {
                                if (orderList[i].Number_of_lines__c != null) {
                                    for (Integer j = 1; j <= Integer.valueof(orderList[i].Number_of_lines__c); j++) {
                                        orderAssetList.add(new Order_Asset__c(
                                        type__c = asset.Type__c,
                                        order__c = orderList[i].Id,
                                        Account__c = orderList[i].Account__c,
                                        Name = 'Line'+j,
                                        Requires_Orchestration__c = asset.Requires_Orchestration__c,
                                        Status__c = defaultAssetStatus));
                                    }
                                } else {
                                    orderAssetList.add(new Order_Asset__c(
                                        type__c = asset.Type__c,
                                        order__c = orderList[i].Id,
                                        Account__c = orderList[i].Account__c,
                                        Name = 'Line1',
                                        Requires_Orchestration__c = asset.Requires_Orchestration__c,
                                        Status__c = defaultAssetStatus));
                                }
                            } else {
                                orderAssetList.add(new Order_Asset__c(
                                    type__c = asset.Type__c,
                                    order__c = orderList[i].Id,
                                    Account__c = orderList[i].Account__c,
                                    Name=asset.Type__c,
                                    Requires_Orchestration__c = asset.Requires_Orchestration__c,
                                    Status__c = defaultAssetStatus));
                            }
                            // store the template config lines for this Asset into the Map
                            configTemplateMap.put(orderList[i].Id+'-'+asset.Type__c,
                                                  pTemplateMap.get(orderList[i].product_type__c).Asset_config_Template_Lines__r);
                        }
                    }
                }
            }

            //RU 16/11/2011: get the account record type id for BE Accounts
             RecordType accRecordType = [Select r.SobjectType, r.Name, r.Id, r.DeveloperName From RecordType r where r.SobjectType = 'Account' and r.DeveloperName = 'BE_Account'];
            System.debug('@@@ accRecordType='+accRecordType);
            // -----------
            // Instantiate a process List and an asset configuration list
            List<CSPOFA__Orchestration_Process__c> processList = new List<CSPOFA__Orchestration_Process__c>();
            List <Asset_configuration__c> assetConfigurationList = new List <Asset_configuration__c>();
            if (orderAssetList.size() > 0) {
                // Insert the Assets
                insert orderAssetList;
                //
                // Create the Asset configuration line item list
                List <Asset_configuration__c> orderAssetConfigList = new List <Asset_configuration__c> ();
                // Create the Asset Orchestration container list
                List<Order_Asset__c> assetNeedingProvList = new List<Order_Asset__c>();

                //RU 17/11/2011 :
                Set<Id> assertAcc = new Set<Id>();

                for(Order_Asset__c assetOrd : orderAssetList){
                    assertAcc.add(assetOrd.Account__c);
                }

                Map<Id, Id> assetAccMap = new Map<Id, Id>();
                List<Account> assetAccountList = new List<Account>();

                assetAccountList = [Select Id, RecordTypeId from Account where Id in :assertAcc];
                for(Account assertAccObj : assetAccountList)
                {
                    assetAccMap.put(assertAccObj.Id, assertAccObj.RecordTypeId);
                }


                // Loop through the Assets and
                // - insert an asset provisioning container if one is required
                // - insert relevant config lines based on the template
                for (Order_Asset__c asset : orderAssetList) {
                    // Create the Asset Configurations
                    for (Asset_Config_Template_Line__c config : configTemplateMap.get(asset.order__c+'-'+asset.Type__c)) {
                        String configValue = null;
                        if (config.Asset_type__c == asset.Type__c) {
                            if (config.Related_Order_Field_Name__c != null && orderMap.containsKey(asset.order__c)) {
                                configValue = (String) orderMap.get(asset.order__c).get(config.Related_Order_Field_Name__c);
                            }
                            //
                            orderAssetConfigList.add(
                                new Asset_configuration__c(Type__c = config.Type__c,
                                                           Asset_Type__c = config.Asset_Type__c,
                                                           Order_Asset__c = asset.Id,
                                                           Name=config.Type__c,
                                                           value__c = configValue));
                        }
                    }
                    //
                    // Create a process for every asset
                    //String processName = orderMap.get(asset.Order__c).Product_Type__c+' Process for asset: '+asset.Type__c;
                    // RC 19-08-2011 Changes for Order Group Process optimisation
                    // RU 16/11/2011 Uncommented the code to enable the orchestration process at Order level for specific BE requirements.
                    System.debug('@@@ assetAccMap='+assetAccMap);

                    if(accRecordType.Id == assetAccMap.get(asset.Account__c))
                    {
                        //String processName = 'Process for '+asset.Type__c;

                        String processName = orderMap.get(asset.Order__c).Product_Type__c;
                        System.debug('@@@ processName='+processName);
                        // added for new version of Orchestrator 1.51.
                        Id templateId = [select Id from CSPOFA__Orchestration_Process_Template__c
                                         where  CSPOFA__Process_Type__c = 'New Provide'
                                         and Name = :processName].Id;


                        //
                        processList.add(new CSPOFA__Orchestration_Process__c(Name = processName,
                                                                             CSPOFA__Process_Type__c = 'New Provide',
                                                                             Order__c       = asset.Order__c,
                                                                             CSPOFA__Orchestration_Process_Template__c = templateId
                                                                             ));
                    }
                }
                // Insert the asset Config lines
                if (!orderAssetConfigList.isEmpty())
                    insert orderAssetConfigList;
                // RC 18-08-2011 As above, this list will now always be empty
                // RU 16/11/2011 Uncommented the code to enable the orchestration process at Order level for specific BE requirements.
                if (!processList.isEmpty())
                    insert processList;


                // Call the provisioning helper
                // - this is only done for assets that require automatic provisioning
                //-----OrchestrationProcessHelper.buildAssetOrchestration(orderAssetList, pTemplateList);
                //
                System.debug('******Limits AggregateQueries****'+Limits.getAggregateQueries()+' From '+Limits.getLimitAggregateQueries());
                System.debug('******Limits SOQLQueries****'+Limits.getQueries()+' From '+Limits.getLimitQueries());
                System.debug('******Limits QueryRows****'+Limits.getQueryRows()+' From '+Limits.getLimitQueryRows());
                System.debug('******Limits FutureCalls****'+Limits.getFutureCalls()+' From '+Limits.getLimitFutureCalls());
            }
        } catch(Exception ex) {
            Task newTask = new Task(subject = 'Validation Errors occured while processing this record:',
                                    //Description =  Utility.getSubString(ex.getMessage(), 'FIELD_CUSTOM_VALIDATION_EXCEPTION, ', ': ['),
                                    Description =  'Method createAssetAndConfigLines: '+ex.getMessage(),
                                    Priority = 'Normal',
                                    Status = 'Not Started',
                                    IsReminderSet = true,
                                    ReminderDateTime = System.now()-1,
                                    WhatId =  orderId);
            insert newTask;
        }
    }


    //
    // Insert the new order assets
    public static MAP<String,String> insertNewOrderAssets(Order__c order,
                                                          List<order_asset__c> orderAssetClonedList,
                                                          List<order_asset__c> orderAssetOrigList) {
        // loop through the assets and update the Order Id before inserting them into the DB
        Integer i = 0;
        for (order_asset__c orderAsset : orderAssetClonedList) {
            orderAsset.related_parent_asset__c = orderAssetOrigList[i].Id;
            orderAsset.Order__c = order.Id;
            orderAsset.Account__c = order.Account__c;
            i++;
        }
        // insert the new assets
        insert orderAssetClonedlist;
        // create a Map of of old to new assets
        MAP<String,String> oldToNewAssetMap = new MAP<String,String>();
        for (order_asset__c orderAsset : orderAssetClonedList) {
            oldToNewAssetMap.put(orderAsset.related_parent_asset__c,orderAsset.Id);
        }
        // return the Map
        return oldToNewAssetMap;
    }

    //
    // Insert the new Order line items
    public static List<Asset_Configuration__c> insertNewAssetConfigs(List<Asset_Configuration__c> assetconfigList, Map<String,String> oldToNewAssetMap) {
        // loop through the line items and update the Order Id before inserting them into the DB
        for (Asset_configuration__c assetConfig : assetconfigList) {
            assetConfig.Order_Asset__c = oldToNewAssetMap.get(assetConfig.Order_Asset__c);
        }
        // insert the new line items
        insert assetconfigList;
        //
        return assetconfigList;
    }

    // Cease a parent order and all its related secondary orders
    //@future
    public static void cloneOrderAssetsAndConfigs(List<String> newOrderIdList, List<String> parentOrderIdList) {
        try {
            List<order_asset__c> orderAssetClonedList = new List<order_asset__c>();
            //
            String queryString = null;
            queryString = 'select '+Utility.getSobjectFields('Order__c')+
                ' , (select '+Utility.getSobjectFields('Order_Asset__c')+' from Order_Assets__r) '+
                ' from Order__c where ID in '+Utility.convertListToString(parentOrderIdList);
            List <Order__c> parentOrderList = Database.query(queryString);
            //
            queryString = 'select '+Utility.getSobjectFields('Order__c')+
                ' , (select '+Utility.getSobjectFields('Order_Asset__c')+' from Order_Assets__r) '+
                ' from Order__c where ID in '+Utility.convertListToString(newOrderIdList);
            List <Order__c> newOrderList = Database.query(queryString);
            Map <ID,Order__c> orderMap = new Map <ID,Order__c> ();
            for (Order__c order : newOrderList) {
                orderMap.put(order.id, order);
            }
            //
            Map<String,String> oldToNewAssetAllMap = new Map<String,String>();
            Integer i = 0;
            for (Order__c order : parentOrderList) {
                // Clone a list of assets for this order
                orderAssetClonedList = order.order_assets__r.deepClone(false);
                // Insert the assets associated with the new novated order
                Map<String,String> oldToNewAssetMap = insertNewOrderAssets(newOrderList[i],
                                                                             orderAssetClonedList,
                                                                             order.order_assets__r);
                oldToNewAssetAllMap.putAll(oldToNewAssetMap);
                i++;
            }
            // Get a list of the new order Asset IDs
            List<String> orderIdList = new List<String>();
            for (String elem : oldToNewAssetAllMap.keySet()) {
                orderIdList.add(oldToNewAssetAllMap.get(elem));
            }
            // Get a list of the new assets
            List<Order_Asset__c> newOrderAssetList = [select Id from Order_Asset__c where Id in :orderIdList];
            // Get a list of all configurations to be novated
            List <Asset_Configuration__c> assetConfigList       = new List<Asset_Configuration__c>();
            List <Asset_Configuration__c> assetConfigClonedList = new List<Asset_Configuration__c>();
            assetConfigList = AssetHelper.getAssetConfigurationList(oldToNewAssetAllMap.keySet());
            assetConfigClonedList = assetConfigList.deepClone(false);
            // We are now ready to create the new configurations
            List<Asset_Configuration__c> newAssetConfigList = insertNewAssetConfigs(assetConfigClonedList,oldToNewAssetAllMap);
            List<Asset_Configuration__c> newAssetConfigDetailsList = [select Id, Type__c, Asset_Type__c, order_Asset__r.order__c, order_Asset__r.order__r.product_Type__c from Asset_Configuration__c where id in :newAssetConfigList];
            // Get the product templates
            List <Product_Template__c> pTemplateList =
                [select Id, Product_Type__c, (select Id, Type__c, Requires_Orchestration__c from Asset_Template_Lines__r),
                (select Id, Type__c, Asset_Type__c, Related_Order_Field_Name__c from Asset_config_Template_Lines__r) //,
                //(select Id, Name, Process__c, Asset_Template_Line__r.Type__c,Product_Template__c from Orchestration_Process_Templates__r)
                from Product_Template__c];
            Map <String,Asset_Config_Template_Line__c> cTemplateMap = new Map <String,Asset_Config_Template_Line__c> ();
            for (Product_Template__c pTemplate : pTemplateList) {
                for (Asset_Config_Template_Line__c configTemplate: pTemplate.Asset_config_Template_Lines__r) {
                    cTemplateMap.put(ptemplate.Product_Type__c+'-'+configTemplate.Asset_Type__c+'-'+configTemplate.Type__c,configTemplate);
                }
            }
            // Loop through the configs
            // - check if there are any related fields in the corresponding template
            // - assign the value to the config from the related order
            for (Asset_Configuration__c config : newAssetConfigDetailsList) {
                String cTemplateMapKey = config.order_Asset__r.order__r.Product_Type__c+'-'+config.Asset_Type__c+'-'+config.Type__c;
                if (cTemplateMap.containsKey(cTemplateMapKey)) {
                    Asset_Config_Template_Line__c configTemplate = cTemplateMap.get(cTemplateMapKey);
                    if (configTemplate.Related_Order_Field_Name__c != null) {
                        if (orderMap.containsKey(config.order_Asset__r.order__c)) {
                            config.Value__c = (String) orderMap.get(config.order_Asset__r.order__c).get(configTemplate.Related_Order_Field_Name__c);
                        }
                    }
                }
            }
            update newAssetConfigDetailsList;
            //
            // Call the provisioning helper
            // - this is only done for assets that require automatic provisioning
            //-----OrchestrationProcessHelper.buildAssetOrchestration(newOrderAssetList, pTemplateList);
            //
            System.debug('******Limits AggregateQueries****'+Limits.getAggregateQueries()+' From '+Limits.getLimitAggregateQueries());
            System.debug('******Limits SOQLQueries****'+Limits.getQueries()+' From '+Limits.getLimitQueries());
            System.debug('******Limits QueryRows****'+Limits.getQueryRows()+' From '+Limits.getLimitQueryRows());
            System.debug('******Limits FutureCalls****'+Limits.getFutureCalls()+' From '+Limits.getLimitFutureCalls());
        } catch(Exception ex) {
            Task newTask = new Task(subject = 'Validation Errors occured while processing this record:2',
                                    //Description =  Utility.getSubString(ex.getMessage(), 'FIELD_CUSTOM_VALIDATION_EXCEPTION, ', ': ['),
                                    Description =  'Method cloneOrderAssetsAndConfigs: '+ex.getMessage(),
                                    Priority = 'Normal',
                                    Status = 'Not Started',
                                    IsReminderSet = true,
                                    ReminderDateTime = System.now()-1,
                                    WhatId =  newOrderIdList[0]);
            insert newTask;
        }
    }

    // Cease a parent order and all its related secondary orders
    // @future
    public static void ceaseParentOrder(String orderId, String replacementType, Date replacementOrderStartDate) {
        try {
            // Get the details of the parent Orders to cease
            String queryString = 'select '+Utility.getSobjectFields('Order__c')+
                ' from Order__c where Id = \''+orderId+'\'';
            List <Order__c> orderList = Database.query(queryString);
            //
            //
            List <Task> taskList = new List <Task>();
            //
            for (Order__c order : orderList) {
                order.Order_Status__c        = 'Suspended';
                order.Order_Status_Reason__c = 'Replaced';
                order.Order_Cease_Stage__c   = 'Replacement Active';
                //
                if (replacementType == 'Price Change / Contract Renewal') {
                    order.Cease_Category__c = 'Financial / Admin';
                    order.Cease_Reason__c = 'Price Renegotiation';
                    order.Physical_Cease_Required__c = false;
                } else if (replacementType == 'Price Change / Contract Amendment') {
                    order.Cease_Category__c = 'Financial / Admin';
                    order.Cease_Reason__c = 'Contract Amendment';
                    order.Physical_Cease_Required__c = false;
                } else if (replacementType == 'Regrade Down (Lesser product)' ||
                           replacementType == 'Regrade Up (Higher product)' ||
                           replacementType == 'Down-grade (Same product)' ||
                           replacementType == 'Up-grade (Same product)') {
                    order.Cease_Category__c = 'Regrade';
                    order.Cease_Reason__c = 'Speed regrade';
                    order.Physical_Cease_Required__c = false;
                } else if (replacementType == 'Relocation') {
                    order.Cease_Reason__c = 'Re-location';
                    if (order.Process_Orchestration_Enabled__c) {
                        order.Physical_Cease_Required__c = false;
                    } else {
                        order.Physical_Cease_Required__c = true;
                    }
                }
                //
                if (!order.Process_Orchestration_Enabled__c) {
                    order.desired_cease_date__c   = replacementOrderStartDate;
                }
                order.Order_Release_Date__c = replacementOrderStartDate;
                //
                if (replacementType == 'Price Change / Contract Renewal') {
                    order.Order_Status__c            = 'Closed';
                    order.Order_Status_Reason__c     = 'Price Change / Contract Renewal';
                    order.Customer_Order_End_Date__c = replacementOrderStartDate.addDays(-1);
                }
                //
                if (replacementType == 'Price Change / Contract Amendment') {
                    order.Order_Status__c            = 'Closed';
                    order.Order_Status_Reason__c     = 'Price Change / Contract Amendment';
                    order.Customer_Order_End_Date__c = replacementOrderStartDate.addDays(-1);
                }
                // if automatic provisioning then close the parent order
                if (order.Process_Orchestration_Enabled__c &&
                    (replacementType == 'Down-grade (Same product)' ||
                     replacementType == 'Up-grade (Same product)')) {
                    order.Order_Status__c            = 'Closed';
                    order.Order_Status_Reason__c     = 'Automatic provisioning';
                    order.Cease_Reason_Comment__c      = 'Automatic cease when replacement order is activated';
                    order.Customer_Order_End_Date__c = replacementOrderStartDate.addDays(-1);
                }
                //-------------
                // If a physical cease is required send a task to the Product Delivery Gatekeeper
                if (order.Physical_Cease_Required__c == true) {
                    String ownerId;
                    try {
                        // Get the user Id of the user occupying the Product Delivery Gatekeeper role
                        ownerId = [Select u.Id from User u  where u.UserRole.Name  = 'Product Delivery Gatekeeper'].Id;
                    } catch(Exception ex) {
                        ownerId = UserInfo.getUserId();
                    }
                    //
                    taskList.add(new Task(Description =  'Release physical components and IP addresses (If not kept on the replacement order).',
                                          Priority = 'Normal',
                                          Status = 'Not started',
                                          Subject = 'Release physical components for related order',
                                          IsReminderSet = true,
                                          ReminderDateTime = System.now()+1,
                                          WhatId =  order.Id,
                                          OwnerId = ownerId));
                }
            }
            if (orderList.size() != 0) update orderList;
            if (taskList.size() != 0) insert taskList;
        } catch(Exception ex) {
            Task newTask = new Task(subject = 'Validation Errors occured while processing this record:3',
                                    Description =  'Method ceaseParentOrder: '+ex.getMessage(),
                                    //Description =  Utility.getSubString(ex.getMessage(), 'FIELD_CUSTOM_VALIDATION_EXCEPTION, ', ': ['),
                                    Priority = 'Normal',
                                    Status = 'Not Started',
                                    IsReminderSet = true,
                                    ReminderDateTime = System.now()-1,
                                    WhatId =  orderId);
            insert newTask;
        }
    }

